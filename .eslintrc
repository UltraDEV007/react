{

  "parser": "babel-eslint",

  "env": {
    "es6": true,
    "mocha": true,
    "node": true,
    "browser": true
  },

  "rules": {
    "strict": 0,
    "import/no-extraneous-dependencies": 0,
    "no-underscore-dangle": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  },

  "parserOptions": {
    "ecmaVersion": 7,
    "experimentalObjectRestSpread": true,
    "sourceType": "module"
  },

  "extends": "airbnb",

  "plugins": [
    "react"
  ]

}
