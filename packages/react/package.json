{
  "name": "@accounts/react",
  "version": "0.0.9",
  "description": "React frontend for js-accounts",
  "main": "lib/index.js",
  "scripts": {
    "start": "babel --watch ./src --out-dir ./lib",
    "compile": "babel ./src --out-dir ./lib",
    "flow:check": "flow check",
    "prepublish": "npm run compile",
    "pretest": "npm run lint",
    "test": "npm run testonly",
    "testonly": "jest",
    "coverage": "npm run testonly -- --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/.*.(test|spec)).(js|jsx)$"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/js-accounts/accounts/tree/master/packages/react"
  },
  "keywords": [
    "react",
    "auth",
    "authentication",
    "accounts",
    "users",
    "oauth"
  ],
  "author": "Tim Mikeladze",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/js-accounts/react/issues"
  },
  "homepage": "https://github.com/js-accounts/react#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react": "^1.0.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coveralls": "^2.11.14",
    "istanbul": "^1.1.0-alpha.1",
    "jest": "^18.1.0",
    "mocha": "^3.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.2",
    "webpack-node-externals": "^1.5.4",
    "@kadira/storybook": "^2.21.0"
  },
  "peerDependencies": {
    "@accounts/client": "^0.0.15",
    "@accounts/common": "^0.0.15"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "recompose": "^0.20.2",
    "redux": "^3.6.0",
    "react-form": "^1.2.5"
  }
}
