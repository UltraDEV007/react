{
  "name": "@accounts/react-material-ui",
  "version": "0.0.11",
  "description": "React frontend build with material-ui for js-accounts",
  "main": "lib/index.js",
  "scripts": {
    "start": "babel --watch ./src --out-dir ./lib",
    "compile": "babel ./src --out-dir ./lib",
    "flow:check": "flow check",
    "prepublish": "npm run compile",
    "pretest": "npm run lint",
    "test": "npm run testonly",
    "testonly": "jest",
    "coverage": "npm run testonly -- --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/.*.(test|spec)).(js|jsx)$"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-accounts/react-material-ui.git"
  },
  "keywords": [
    "react",
    "auth",
    "authentication",
    "accounts",
    "users",
    "material"
  ],
  "author": "Tim Mikeladze",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/js-accounts/react-material-ui/issues"
  },
  "homepage": "https://github.com/js-accounts/react-material-ui#readme",
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.3.0",
    "@kadira/storybook": "^2.30.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.0",
    "img-loader": "^1.3.1",
    "istanbul": "^1.1.0-alpha.1",
    "jest": "^18.1.0",
    "material-ui": "^0.17.3",
    "mocha": "^3.1.2",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "storybook-addon-material-ui": "^0.7.3",
    "webpack": "^1.13.3",
    "webpack-node-externals": "^1.5.4"
  },
  "peerDependencies": {
    "@accounts/client": "^0.0.15",
    "@accounts/common": "^0.0.15",
    "material-ui": "^0.16.4"
  },
  "dependencies": {
    "@accounts/react": "^0.0.11",
    "flexbox-react": "^4.1.0",
    "prop-types": "^15.5.8",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.6",
    "recompose": "^0.23.1"
  }
}
